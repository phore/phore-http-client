{
  "name": "phore/http-client",
  "description": "Http client library for fast api access",
  "keywords": ["Keyword1", "Keyword2"],
  "homepage": "https://infracamp.org/phore/",
  "license": "MIT",
  "authors": [
    { "name": "Matthias Leuffen", "email": "m@tth.es" }
  ],
  "repositories": [
  ],
  "require": {
    "php": ">=7.1",
    "phore/core": "*",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Phore\\HttpClient\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "suggest": {
    "phore/cache": "Use request caching",
    "phore/service-exception": "Use service exception handling"
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "*",
    "phore/cache": "*",
    "phore/filesystem": "*",
    "phore/objectstore": "*"
  }
}
